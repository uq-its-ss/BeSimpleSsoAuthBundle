<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="be_simple.sso_auth.saml_config.class">BeSimple\SsoAuthBundle\Sso\Saml\Config</parameter>
    </parameters>

    <services>
        <service id="BeSimple\SsoAuthBundle\Sso\Saml\Config" public="true">
            <tag name="be_simple.sso_auth.saml_config" id="saml"/>
        </service>
        <service id="be_simple.sso_auth.saml_config" alias="BeSimple\SsoAuthBundle\Sso\Saml\Config"/>
    </services>
</container>
